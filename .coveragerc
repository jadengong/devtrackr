[run]
source = .
omit = 
    */tests/*
    */venv/*
    */.venv/*
    */__pycache__/*
    */migrations/*
    */alembic/*
    setup.py
    conftest.py
    test_*.py
    run_tests.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod
